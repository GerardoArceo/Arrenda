{
  "name": "arrenda-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4330",
    "start:prod": "ng run build:prod && npm run http-server",
    "build": "ng build --prod",
    "build:prod": "ng build -c=build_prod --outputPath=dist/prod/ --output-hashing none",
    "build:prod1": "ng build -c=build_prod1 --outputPath=dist/prod1/",
    "build:uat": "ng build -c=build_uat --outputPath=dist/uat/",
    "build:dev": "ng build -c=build_dev --outputPath=dist/dev/",
    "deploy": "ng deploy --baseHref=/angular-basic/",
    "test": "ng test --karma-config=karma.conf.js --no-progress --watch=false --code-coverage=true --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@citi-icg-158830/elemental-chameleon": "^1.5.0",
    "@material-ui/core": "^4.11.3",
    "@schematics/angular": "^11.0.5",
    "@schematics/update": "^0.1100.5",
    "angular-dual-listbox": "^5.0.1",
